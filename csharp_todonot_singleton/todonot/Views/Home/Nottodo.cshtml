@{
    //Layout = null;
}

<h1>@ViewBag.Message</h1>

<h2>@Model.Name</h2>


<table class="table table-striped table-bordered">
    <tr>
        <th>DO NOT</th>
        <th>Created Date</th>
        <th>Did I do that?</th>
    </tr>
@foreach (var todo in Model.Todos) {   
    <tr>
        <td>@todo.Text</td>
        <td>@todo.dateCompleted</td>
        <td>@todo.isCompleted</td>
    </tr>
} 
</table>